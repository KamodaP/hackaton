<html metal:use-macro="load: main.pt">

<div metal:fill-slot="content">
<div class = "container">
<table>
	<thead>
		<tr>
			<th data-field="name">Name</th>
			<th data-field="owner_id">Owner</th>
			<th data-field="tags">Tags</th>
		</tr>
	</thead>
	<tbody>
		<tr tal:repeat="item game_records">
			<td>${item.name}</td>
			<td>${item.owner}</td>
			<td>${item.tags}</td>
		</tr>
	</tbody>
</table>
</div>
</div>
</html>